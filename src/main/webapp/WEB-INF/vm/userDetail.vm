<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>Title</title>
    <link rel="stylesheet" href="/static/js/jquery-easyui-1.5.4.5/themes/default/easyui.css">
    <link rel="stylesheet" href="/static/js/jquery-easyui-1.5.4.5/themes/icon.css">
    <link rel="stylesheet" href="/static/js/layui/css/layui.css">
    <link rel="stylesheet" href="/static/css/style.css">
    <script src="/static/jquery-easyui-1.5.4.5/jquery.min.js"></script>
    <script src="/static/jquery-easyui-1.5.4.5/jquery.easyui.min.js"></script>
    <script src="/static/jquery-easyui-1.5.4.5/locale/easyui-lang-zh_CN.js"></script>
    <script src="/static/layui/layui.js"></script>
</head>
    #set($pageId = "userDetail")
<body>
<form method="post" id="userDetailForm">
    <div style="padding: 13px">
        <laber >用户：</laber>
        <input type="text" class="easyui-validatebox easyui-textbox" name="userName" data-options="required:true">
    </div>
    <div style="padding: 13px">
        <laber >密码：</laber>
        <input type="text" class="easyui-validatebox easyui-textbox" name="passWord" data-options="required:true">
    </div>
</form>
<div class="easyui-panel" title="功能">
    <div class="" style="padding: 3px;margin-left: 5px">
        <a id="${pageId}BtnSave" href="#" class="easyui-linkbutton" iconCls='icon-save'>保存</a>
        <a id="${pageId}BtnReset" href="#" class="easyui-linkbutton" iconCls='icon-redo'>重置</a>
        <a id="${pageId}BtnQuit" href="#" class="easyui-linkbutton" iconCls='icon-back'>退出</a>
    </div>
</div>
<script src="/static/js/userDetail.js"></script>
<script src="/static/js/common/cldCommon.js"></script>

</body>
</html>
